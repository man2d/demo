<% if @item.interior_assets.size > 0 %>
        <div class="imageflow" id="exteriorFlow">
			<% @item.interior_assets.each do |asset| %>
            <img class="item" alt="" title="" src="<%= asset.asset.url(:gallery) %>" longdesc="<%= asset.asset.url(:big) %>" />
            <% end %>
        </div>
<% end %>