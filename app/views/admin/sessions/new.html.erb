<h2>Вход в защищенную зону</h2>

<%= form_for("admin_user", :url => admin_user_session_path) do |f| %>
  <p><%= f.label :email, "E-mail" %><br />
  <%= f.text_field :email %></p>

  <p><%= f.label :password, "Пароль" %><br />
  <%= f.password_field :password %></p>

  <% if devise_mapping.rememberable? -%>
    <p><%= f.check_box :remember_me %> <%= f.label :remember_me, "Запомнить меня" %></p>
  <% end -%>

  <p><%= f.submit "Войти" %></p>
<% end %>

<%#= render :partial => "devise/shared/links" %>