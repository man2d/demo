<body <%=raw 'class="transparent"' if @template == 2 %>>
	<%#= current_user.to_xml if current_user %>
<div class="wrap"><div class="mapBg"><%=raw '<img id="qwe2" src="/images/sunset.jpg" alt="">' if @template == 2 %></div>
<div id="main_container"><div class="contMargin bg"><div class="bgPic"></div>
	<div class="head">
		<div class="w910">
		<% if params[:controller] == 'home' && params[:action] == 'index' %>
    	<p class="logo"></p>
		<% else %>
		<a href="/" class="logo"></a>
		<% end %>
        <div class="right">
        	<div class="right clear">
            	<a href="/" class="home"></a>
                <a href="/contacts/" class="mail"></a>
                <a href="/sitemap/" class="sitemap"></a>
            </div>
            <div class="right clear">

        			<div class="phone color_gray_l">(812) <a>320-63-00</a>
						<div class="shadow mapPos" style="display:none;"><div class="b"><div class="bl"></div><div class="tr"><div class="r"></div>
						<div class="dataShadowWrap smallForm map">
								<p class="phone">(812) 320-63-00</p>
									<%= form_tag "/messages/contact" %>								
									<h2 class="color_a">Центральный офис</h2>
									<p>
										194156, Россия, Санкт-Петербург,<br />ул. Зеленогорская, д. 4, лит. А<br /><br />

										Мы работаем с понедельника<br />
										по субботу с 9:00 до 18:30<br /><br />

										Тел.: (812) 320-63-00,<br />
										факс:  (812) 320-63-00
									</p>
									<a class="psevdo sendMsg" onclick="$('#sendMsg').slideToggle();">Отправить сообщение</a>

									<div id="sendMsg">
										<div class="text">
											<label for="name">Представьтесь, пожалуйста</label>
											<div class="clear"></div>
											<input type="text" id="name" name="name" value="<%= current_user.name if current_user %>" />
										</div>
										<div class="text">
											<label for="mail">Эл. почта</label>
											<div class="clear"></div>
											<input type="text" id="mail" name="email" value="<%= current_user.email if current_user %>" />
										</div>
										<div class="text">
											<label>Сообщение:</label>
											<div class="clear"></div>
											<textarea name="message"></textarea>
										</div>
										<input class="submit" value="Отправить" type="submit" /><p class="error"></p>
									</div>
								</form>
						<a class="close"></a></div></div></div><div class="arrow"></div></div>
					</div>
				<% unless current_user %>
				<div class="client" style="z-index:2000;"><a class="enter" href="/login" unselectable="on" onclick="return false;">Войдите</a> или <a href="/register">зарегистрируйтесь</a>.
					<div class="shadow enterFormPos" style="display:none;"><div class="b"><div class="bl"></div><div class="tr"><div class="r"></div>
					<div class="dataShadowWrap enterForm smallForm">
	<%= form_tag("/users/sign_in", :class => [:validatable]) do |f| %>
								<div class="text">
									<label for="mail">Эл. почта</label>
									<div class="clear"></div>
									<input type="text" id="user_email" name="user[email]" />
								</div>
								<div class="text">
									<label for="pass">Пароль</label>
									<div class="clear"></div>
									<input type="password" id="user_password" name="user[password]" />
								</div>
								<input class="submit" value="Войти" type="submit" /><!--<p class="error">Неверный пароль или эл. почта</p>-->
	<% end %>
					</div></div></div></div>
			    <% else %>
			     <div class="client"><a class="name" href="/users/<%= current_user.id %>"><span><%= current_user.name %></span></a><a class="enter" href="/logout">Выход</a>
			
				<% end %>
				
	</div>
				
            </div>
			</div>
			<div id="compare_container"><%= render 'comparison/list' %></div>
			<ul class="main_menu" style="height: 30px;">
			<%= render 'shared/menu' %>

		        <li style="z-index: 24;" class="blog"><a href="/blogs/"<%=raw ' class="current"' if params[:controller] == 'blogs' %>>БЛОГ</a><span></span></li>
		    </ul>
		</div>
    </div>
    
    <div id="content">

	  <%= yield %>
		<div class="yacht">
			<div class="pict"><a class="yachtPic"></a>
			<div class="shadow yacht_tip_container" style=""><div class="b"><div class="bl"></div><div class="tr"><div class="r"></div>
			<div class="dataShadowWrap yacht_tip_data">
					<div class="yacht_tip_dataWrap">
						
					</div>
			<a unselectable="on" class="close"></a><a unselectable="on" class="refresh"></a></div></div></div><div class="arrow"></div></div>
			</div>
			<div class="dataShadowWrap yacht_tip_data">
					<div class="tempTipData"></div>
			</div>
          <div class="water"></div>
      </div>
    </div>
</div>
